{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "author": "leonardomso",
  "license": "MIT",
  "scripts": {
    "pull": "dotenv -e .env.development prisma db pull",
    "push": "dotenv -e .env.development prisma db push",
    "start": "ts-node-dev --transpile-only --no-notify ./src/index.ts",
    "build": "tsc",
    "generate": "prisma generate",
    "generate:prisma": "prisma generate",
    "lint": "eslint",
    "format": "prettier --write src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules"
  },
  "dependencies": {
    "@envelop/core": "^2.6.0",
    "@envelop/disable-introspection": "^3.6.0",
    "@envelop/graphql-jit": "^4.6.0",
    "@envelop/parser-cache": "^4.7.0",
    "@envelop/response-cache": "^3.2.0",
    "@envelop/validation-cache": "^4.7.0",
    "@fastify/cors": "^8.1.0",
    "@novu/node": "^0.8.0",
    "@pothos/core": "^3.22.5",
    "@pothos/plugin-errors": "^3.8.4",
    "@pothos/plugin-prisma": "^3.35.4",
    "@pothos/plugin-relay": "^3.28.4",
    "@pothos/plugin-simple-objects": "^3.5.0",
    "@pothos/plugin-tracing": "^0.5.4",
    "@pothos/plugin-validation": "^3.8.4",
    "@prisma/client": "^4.4.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.2",
    "fastify": "^4.7.0",
    "graphql": "^16.6.0",
    "graphql-helix": "^1.13.0",
    "graphql-playground-html": "^1.6.30",
    "graphql-relay": "^0.10.0",
    "graphql-scalars": "^1.18.0",
    "jsonwebtoken": "^8.5.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.8.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.25.0",
    "eslint-config-xo": "^0.42.0",
    "prisma": "^4.4.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
}
